{
  "extends": [
    "airbnb",
    "airbnb-typescript/base",
    "plugin:prettier/recommended",
    "plugin:import"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/indent": [
      "error",
      "tab"
    ],
    "strict": 0
  }
}